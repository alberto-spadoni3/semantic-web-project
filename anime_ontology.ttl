#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ontology/anime/animeInMeteorologicalSeason
:animeInMeteorologicalSeason rdf:type owl:ObjectProperty ;
                             owl:inverseOf :meteorologicalSeason .


###  http://www.semanticweb.org/ontology/anime/animeWithAudience
:animeWithAudience rdf:type owl:ObjectProperty ;
                   owl:inverseOf schema:audience .


###  http://www.semanticweb.org/ontology/anime/animeWithCharacter
:animeWithCharacter rdf:type owl:ObjectProperty ;
                    owl:inverseOf schema:character .


###  http://www.semanticweb.org/ontology/anime/animeWithContentRating
:animeWithContentRating rdf:type owl:ObjectProperty ;
                        owl:inverseOf schema:contentRating .


###  http://www.semanticweb.org/ontology/anime/animeWithGenre
:animeWithGenre rdf:type owl:ObjectProperty ;
                owl:inverseOf schema:genre .


###  http://www.semanticweb.org/ontology/anime/animeWithSource
:animeWithSource rdf:type owl:ObjectProperty ;
                 owl:inverseOf schema:isBasedOn .


###  http://www.semanticweb.org/ontology/anime/animeWithStatus
:animeWithStatus rdf:type owl:ObjectProperty ;
                 owl:inverseOf :creativeWorkStatus .


###  http://www.semanticweb.org/ontology/anime/creativeWorkStatus
:creativeWorkStatus rdf:type owl:ObjectProperty ;
                    rdfs:domain schema:CreativeWork ;
                    rdfs:range :Status .


###  http://www.semanticweb.org/ontology/anime/hasPrequel
:hasPrequel rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasSequel .


###  http://www.semanticweb.org/ontology/anime/hasRelation
:hasRelation rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain dbo:Anime ;
             rdfs:range dbo:Anime .


###  http://www.semanticweb.org/ontology/anime/hasSequel
:hasSequel rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasRelation ;
           rdfs:domain dbo:Anime ;
           rdfs:range dbo:Anime .


###  http://www.semanticweb.org/ontology/anime/hasSpinOff
:hasSpinOff rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasRelation ;
            owl:inverseOf :isSpinOffOf ;
            rdfs:domain dbo:Anime ;
            rdfs:range dbo:Anime .


###  http://www.semanticweb.org/ontology/anime/hasVoiceActor
:hasVoiceActor rdf:type owl:ObjectProperty ;
               owl:inverseOf :voiceActed ;
               rdfs:domain schema:Person ;
               rdfs:range dbo:VoiceActor .


###  http://www.semanticweb.org/ontology/anime/isSpinOffOf
:isSpinOffOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.semanticweb.org/ontology/anime/isStreamedOn
:isStreamedOn rdf:type owl:ObjectProperty ;
              owl:inverseOf :providesContentFor ;
              rdfs:domain dbo:Anime ;
              rdfs:range schema:BroadcastEvent .


###  http://www.semanticweb.org/ontology/anime/languageOfVoicing
:languageOfVoicing rdf:type owl:ObjectProperty ;
                   owl:inverseOf :languageVoicedBy ;
                   rdfs:domain dbo:VoiceActor ;
                   rdfs:range schema:Language .


###  http://www.semanticweb.org/ontology/anime/languageVoicedBy
:languageVoicedBy rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ontology/anime/meteorologicalSeason
:meteorologicalSeason rdf:type owl:ObjectProperty ;
                      rdfs:domain :AnimeSeason .


###  http://www.semanticweb.org/ontology/anime/producedAnime
:producedAnime rdf:type owl:ObjectProperty ;
               owl:inverseOf schema:producer .


###  http://www.semanticweb.org/ontology/anime/providesContentFor
:providesContentFor rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ontology/anime/voiceActed
:voiceActed rdf:type owl:ObjectProperty .


###  https://schema.org/audience
schema:audience rdf:type owl:ObjectProperty ;
                rdfs:domain schema:CreativeWork ;
                rdfs:range schema:Audience .


###  https://schema.org/character
schema:character rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain dbo:Anime ;
                 rdfs:range schema:Person .


###  https://schema.org/contentRating
schema:contentRating rdf:type owl:ObjectProperty ;
                     rdfs:domain schema:CreativeWork ;
                     rdfs:range schema:Rating .


###  https://schema.org/creativeWorkStatus
schema:creativeWorkStatus rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf schema:character ;
                          rdfs:domain schema:CreativeWork ;
                          rdfs:range schema:Text .


###  https://schema.org/episode
schema:episode rdf:type owl:ObjectProperty ;
               owl:inverseOf schema:partOfSeason ;
               rdfs:domain schema:CreativeWorkSeason ;
               rdfs:range schema:Episode .


###  https://schema.org/genre
schema:genre rdf:type owl:ObjectProperty ;
             rdfs:domain schema:CreativeWork .


###  https://schema.org/inLanguage
schema:inLanguage rdf:type owl:ObjectProperty ;
                  rdfs:domain schema:BroadcastEvent ,
                              schema:CreativeWork ;
                  rdfs:range schema:Language .


###  https://schema.org/isBasedOn
schema:isBasedOn rdf:type owl:ObjectProperty ;
                 rdfs:domain schema:CreativeWork ;
                 rdfs:range dbo:Manga ,
                            schema:Book .


###  https://schema.org/partOfSeason
schema:partOfSeason rdf:type owl:ObjectProperty .


###  https://schema.org/producer
schema:producer rdf:type owl:ObjectProperty ;
                rdfs:domain schema:CreativeWork ;
                rdfs:range schema:Organization .


###  https://schema.org/publishedOn
schema:publishedOn rdf:type owl:ObjectProperty ;
                   rdfs:domain schema:PublicationEvent ;
                   rdfs:range schema:BroadcastService ;
                   rdfs:comment "A broadcast service associated with the publication event." ;
                   rdfs:label "publishedOn" .


###  https://schema.org/subtitleLanguage
schema:subtitleLanguage rdf:type owl:ObjectProperty ;
                        rdfs:domain schema:BroadcastEvent ,
                                    schema:Movie ;
                        rdfs:range schema:Language ;
                        rdfs:comment "\"Languages in which subtitles/captions are available, in IETF BCP 47 standard format (http://tools.ietf.org/html/bcp47)." ;
                        rdfs:label "subtitleLanguage" .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/alternative
dct:alternative rdf:type owl:DatatypeProperty ;
                owl:equivalentProperty schema:alternateName ;
                rdfs:subPropertyOf dct:title ;
                rdfs:range rdfs:Literal ;
                dct:description "The distinction between titles and alternative titles is application-specific."@en ;
                rdfs:comment "An alternative name for the resource."@en ;
                rdfs:isDefinedBy dct: ;
                rdfs:label "Alternative Title"@en .


###  http://purl.org/dc/terms/date
dct:date rdf:type owl:DatatypeProperty ;
         rdfs:range rdfs:Literal ,
                    xsd:dateTime ;
         dct:description "Date may be used to express temporal information at any level of granularity.  Recommended practice is to express the date, date/time, or period of time according to ISO 8601-1 [[ISO 8601-1](https://www.iso.org/iso-8601-date-and-time-format.html)] or a published profile of the ISO standard, such as the W3C Note on Date and Time Formats [[W3CDTF](https://www.w3.org/TR/NOTE-datetime)] or the Extended Date/Time Format Specification [[EDTF](http://www.loc.gov/standards/datetime/)].  If the full date is unknown, month and year (YYYY-MM) or just year (YYYY) may be used. Date ranges may be specified using ISO 8601 period of time specification in which start and end dates are separated by a '/' (slash) character.  Either the start or end date may be missing."@en ;
         rdfs:comment "A point or period of time associated with an event in the lifecycle of the resource."@en ;
         rdfs:isDefinedBy dct: ;
         rdfs:label "Date"@en .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:DatatypeProperty ;
          owl:equivalentProperty schema:name ;
          rdfs:range rdfs:Literal ;
          rdfs:comment "A name given to the resource."@en ;
          rdfs:isDefinedBy dct: ;
          rdfs:label "Title"@en .


###  http://www.semanticweb.org/ontology/anime/meteorologicalSeason
:meteorologicalSeason rdf:type owl:DatatypeProperty ;
                      rdfs:range rdfs:Literal .


###  https://schema.org/airingEndDate
schema:airingEndDate rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf dct:date ;
                     rdfs:domain schema:CreativeWork ,
                                 schema:CreativeWorkSeason ;
                     rdfs:range xsd:dateTime .


###  https://schema.org/airingStartDate
schema:airingStartDate rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf dct:date ;
                       rdfs:domain schema:CreativeWork ,
                                   schema:CreativeWorkSeason ;
                       rdfs:range xsd:dateTime .


###  https://schema.org/alternateName
schema:alternateName rdf:type owl:DatatypeProperty ;
                     rdfs:domain schema:Thing ;
                     rdfs:range rdfs:Literal ;
                     rdfs:comment "An alias for the item." ;
                     rdfs:label "alternateName" .


###  https://schema.org/audienceType
schema:audienceType rdf:type owl:DatatypeProperty ;
                    rdfs:domain schema:PeopleAudience ;
                    rdfs:range rdfs:Literal .


###  https://schema.org/datePublished
schema:datePublished rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf dct:date ;
                     rdfs:domain schema:CreativeWork ;
                     rdfs:range xsd:dateTime .


###  https://schema.org/description
schema:description rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain schema:Thing ;
                   rdfs:range rdfs:Literal ;
                   rdfs:comment "A description of the item." ;
                   rdfs:label "description" .


###  https://schema.org/duration
schema:duration rdf:type owl:DatatypeProperty ;
                rdfs:domain schema:Movie ;
                rdfs:range rdfs:Literal .


###  https://schema.org/episodeNumber
schema:episodeNumber rdf:type owl:DatatypeProperty ;
                     rdfs:domain schema:Episode ;
                     rdfs:range xsd:int .


###  https://schema.org/genre
schema:genre rdf:type owl:DatatypeProperty ;
             rdfs:range rdfs:Literal .


###  https://schema.org/name
schema:name rdf:type owl:DatatypeProperty ;
            rdfs:domain owl:Thing ;
            rdfs:range rdfs:Literal .


###  https://schema.org/numberOfEpisodes
schema:numberOfEpisodes rdf:type owl:DatatypeProperty ;
                        rdfs:domain schema:CreativeWorkSeason ;
                        rdfs:range xsd:int .


###  https://schema.org/suggestedGender
schema:suggestedGender rdf:type owl:DatatypeProperty ;
                       rdfs:domain schema:PeopleAudience ;
                       rdfs:range rdfs:Literal ;
                       rdfs:comment "The suggested gender of the intended person or audience, for example \\\"male\\\", \\\"female\\\", or \\\"unisex\\\"." ;
                       rdfs:label "suggestedGender" .


###  https://schema.org/suggestedMaxAge
schema:suggestedMaxAge rdf:type owl:DatatypeProperty ;
                       rdfs:domain schema:PeopleAudience ;
                       rdfs:range xsd:int ;
                       rdfs:comment "Maximum recommended age in years for the audience or user"@en ;
                       rdfs:label "suggestedMaxAge"@en .


###  https://schema.org/suggestedMinAge
schema:suggestedMinAge rdf:type owl:DatatypeProperty ;
                       rdfs:domain schema:PeopleAudience ;
                       rdfs:range xsd:int ;
                       rdfs:comment "Minimum recommended age in years for the audience or user."@en ;
                       rdfs:label "suggestedMinAge"@en .


###  https://schema.org/url
schema:url rdf:type owl:DatatypeProperty ;
           rdfs:domain schema:Thing ;
           rdfs:range rdfs:Literal ;
           rdfs:comment "URL of the item." ;
           rdfs:label "url" .


